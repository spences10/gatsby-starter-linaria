{
  "name": "gatsby-starter-linaria",
  "private": true,
  "description": "A starter for Gatsby using Linaria",
  "version": "0.1.0",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "linaria"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "pretty-quick",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-offline": "^3.2.9",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sharp": "^2.6.11",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-sharp": "^2.5.5",
    "linaria": "^2.0.0-alpha.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "prettier": "2.6.2",
    "pretty-quick": "^2.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5",
    "printWidth": 70,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/gatsby-starter-linaria.git"
  },
  "bugs": {
    "url": "https://github.com/spences10/gatsby-starter-linaria/issues"
  },
  "homepage": "https://github.com/spences10/gatsby-starter-linaria#readme"
}
